{
  "name": "contraction-timer",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://rjw1428.github.io/Eurika",
  "description": "A simple & free contraction timer for tracking the duration and frequency of contractions during pregnancy labor.",
  "scripts": {
    "dev": "vite",
